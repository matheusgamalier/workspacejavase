package br.com.universidade.modelo;

public class Professor {
	private int id;
	private String nome;
	private float valorHora;
	private String area;
	
	public void setNome(String nome) { this.nome = nome.toUpperCase(); }
	
	public void setValorHora(String valorHora) { this.valorHora = valorHora; }
	
	public void setRm(int rm) {	this.rm = rm; }
	
	public String getNome() { return this.nome; }
	
	public String getValorHora() { return this.ValorHora; }
	
	public int getRm() { return this.rm; }
}
