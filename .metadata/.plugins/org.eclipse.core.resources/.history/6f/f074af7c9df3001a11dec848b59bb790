package br.com.universidade.implementacao;

import javax.swing.JOptionPane;

import br.com.universidade.modelo.Professor;

public class TesteProfessor {

	public static void main(String[] args) {
		Professor p = new Professor();
		
		p.setId(Integer.parseInt(JOptionPane.showInputDialog("Digite o RM")));
		p.setNome(JOptionPane.showInputDialog("Digite o nome"));
		p.setArea(JOptionPane.showInputDialog("Digite o email"));
		p.setValorHora(Float.parseFloat(JOptionPane.showInputDialog("Digite o valor hora")));
		
		
		JOptionPane.showMessageDialog(null, "Id: " + p.getId());
		JOptionPane.showMessageDialog(null, "Nome: " + p.getNome());
		JOptionPane.showMessageDialog(null, "Email: " + p.getArea());
		JOptionPane.showMessageDialog(null, "RM: " + p.getValorHora());

	}

}
