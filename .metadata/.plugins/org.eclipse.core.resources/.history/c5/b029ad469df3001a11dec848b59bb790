package br.com.universidade.implementacao;

import javax.swing.JOptionPane;

import br.com.universidade.modelo.Professor;

public class TesteProfessor {

	public static void main(String[] args) {
		Professor p = new Professor();
		
		p.setNome(JOptionPane.showInputDialog("Digite o nome"));
		p.setEmail(JOptionPane.showInputDialog("Digite o email"));
		p.setRm(Integer.parseInt(JOptionPane.showInputDialog("Digite o RM")));
		
		JOptionPane.showMessageDialog(null, "Nome: " + p.getNome());
		JOptionPane.showMessageDialog(null, "Email: " + p.getEmail());
		JOptionPane.showMessageDialog(null, "RM: " + p.getRm());

	}

}
